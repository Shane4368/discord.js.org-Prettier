{
    "manifest_version": 3,
    "name": "discord.js.org Prettier",
    "description": "Reduce padding, change colours, etc.",
    "author": "Shane4368",
    "version": "0.2.2",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://discord.js.org/*"],
            "css": [
                "prettier.css"
            ]
        }
    ]
}